    "import time\n",
    "\n",
    "def task(id, delay):\n",
    "    print(f\"開始任務 {id}\")\n",
    "    time.sleep(delay)\n",
    "    print(f\"任務 {id} 完成\")\n",
    "\n",
    "start = time.time()\n",
    "task(1, 1)\n",
    "task(2, 2)\n",
    "print(f\"總耗時: {time.time() - start:.2f} 秒\")"
   ]
  }
